{
  "name": "dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "test": "jest",
    "update-snapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.12.1",
    "@popperjs/core": "^2.9.2",
    "@turf/turf": "^6.3.0",
    "@visx/axis": "^1.7.0",
    "@visx/group": "^1.7.0",
    "@visx/mock-data": "^1.7.0",
    "@visx/scale": "^1.7.0",
    "@visx/shape": "^1.7.0",
    "downshift": "^6.1.2",
    "lru-cache": "^6.0.0",
    "mapbox-gl": "^2.2.0",
    "mathjax-react": "^1.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-collapsible": "^2.8.3",
    "react-dom": "^16.9.0",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.17",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3",
    "use-debounce": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-css": "^2.1.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.3"
  },
  "jest": {
    "automock": false,
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.css$": "jest-transform-css",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|geojson)$": "<rootDir>/src/utils/fileTransformer.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/src/utils/fileMock.js"
    },
    "testEnvironment": "jsdom"
  }
}
